{
  "name": "gmsoft-front-plugin",
  "displayName": "gmsoft-front-plugin",
  "description": "Gmsoft Front Plugin",
  "version": "0.0.1",
  "publisher": "gmsoft-front-plugin",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gmsoft-front-plugin.order-imports",
        "title": "Order Imports"
      }
    ],
    "keybindings": [
      {
        "command": "gmsoft-front-plugin.order-imports",
        "key": "ctrl+shift+Z",
        "mac": "cmd+shift+Z"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run build",
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src",
    "test": "jest",
    "prepackage": "tsup src/extension.ts --format cjs --external vscode --no-shims",
    "package": "vsce package --no-dependencies",
    "prepublish": "tsup src/extension.ts --format cjs --external vscode --no-shims",
    "publish": "vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@babel/types": "^7.26.0",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.6",
    "@types/jest": "^29.5.14",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@babel/generator": "^7.26.2",
    "@babel/parser": "^7.26.2",
    "@babel/traverse": "^7.25.9"
  }
}
